@model PROGESO.Models.EncDepartamento
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Tittle</h2>
<br />
</div>
<!-- Start header -->
<div class="all-page-title page-breadcrumb">
    <div class="container text-center">
        <div class="row">
            <div class="col-lg-12">
                <h1>
                    Eliminar Encargado
                </h1>
            </div>
        </div>
    </div>
</div>
<h2 class="text-info"></h2>
<br />

@using (Html.BeginForm("Delete", "EncDepartamento", FormMethod.Post))
{
<div class="form-group-row">
    <div class="col-md-6">
        @Html.HiddenFor(d => d.Id)
        <div>
            <label for="Id">Id</label>
            @Html.TextBoxFor(d => d.Id, "", new { @class = "form-control" , @readonly = "true" })
            @Html.ValidationMessageFor(d => d.Id, null, new { @class = "red" })
        </div>
        <div>
            <label for="Correo">Correo</label>
            @Html.TextBoxFor(d => d.Correo, "", new { @class = "form-control" , @readonly = "true" })
            @Html.ValidationMessageFor(d => d.Correo, null, new { @class = "red" })
        </div>
        <div>
            <label for="Nombre">Nombre</label>
            @Html.TextBoxFor(d => d.Nombre, "", new { @class = "form-control" , @readonly = "true" })
            @Html.ValidationMessageFor(d => d.Nombre, null, new { @class = "red" })
        </div>

        <div>
            <label for="Apellido1">Apellido1</label>
            @Html.TextBoxFor(d => d.Apellido1, "", new { @class = "form-control" , @readonly = "true" })
            @Html.ValidationMessageFor(d => d.Apellido1, null, new { @class = "red" })
        </div>
        <div>
            <label for="Apellido1">Apellido2</label>
            @Html.TextBoxFor(d => d.Apellido2, "", new { @class = "form-control" , @readonly = "true" })
            @Html.ValidationMessageFor(d => d.Apellido2, null, new { @class = "red" })
        </div>
        <div>
            <label for="DepartamentID">Departamento</label>
            @Html.TextBoxFor(d => d.DepartamentID, "", new { @class = "form-control", @readonly = "true" })
            @Html.ValidationMessageFor(d => d.DepartamentID, null, new { @class = "red" })
        </div>


        <br />

    </div>


</div>
    <div class="col-6">
        @Html.Partial("_DeleteAndBackToListButton")
    </div>
}